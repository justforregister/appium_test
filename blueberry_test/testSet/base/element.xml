<?xml version="1.0" encoding="UTF-8"?>
<page>

    <!--导航页面 -->
    <activity name = "MallHomeActivity"><!--activity名称-->

        <element name="City">
            <name>city_text</name>
            <type>TextView</type>
            <pathtype>ID</pathtype>
            <pathvalue>city_text</pathvalue>
        </element>
        
        <element name="welcome"> <!-- 元素对象 -->
            <name>Welcome</name><!-- 元素名称 -->
            <type>RelativeLayout</type><!-- 元素类型 input/button -->
            <pathtype>ID</pathtype><!-- 获取元素的模式 ID/XPATH/CLASSNAME/NAME-->
            <pathvalue>ag_ll_dotlayout</pathvalue><!-- 元素定位路径 -->
        </element>

        <element name="goShop"> <!-- 元素对象 -->
            <name>GoShop</name><!-- 元素名称 -->
            <type>button</type><!-- 元素类型 input/button -->
            <pathtype>ID</pathtype><!-- 获取元素的模式 ID/XPATH/CLASSNAME/NAME-->
            <pathvalue>agi_bn_goshop</pathvalue><!-- 元素定位路径 -->
        </element>

        <element name="skip"> <!-- 元素对象 -->
            <name>Skin</name><!-- 元素名称 -->
            <type>button</type><!-- 元素类型 input/button -->
            <pathtype>ID</pathtype><!-- 获取元素的模式 ID/XPATH/CLASSNAME/NAME-->
            <pathvalue>skipBtn</pathvalue><!-- 元素定位路径 -->
        </element>
    </activity>

    <!--所有的弹出框和加载-->
    <activity name="Alert">

        <element name="cancel">
            <name>Cancel</name>
            <type>button</type>
            <pathtype>ID</pathtype>
            <pathvalue>cancel_btn</pathvalue>
        </element>

        <element name="confirm">
            <name>Confirm</name>
            <type>button</type>
            <pathtype>ID</pathtype>
            <pathvalue>confirm_btn</pathvalue>
        </element>

        <element name="loading">
            <name>Loading</name>
            <type>ProgressBar</type>
            <pathtype>CLASSNAME</pathtype>
            <pathvalue>android.widget.ProgressBar</pathvalue>
        </element>

    </activity>

    <!--Shop-->
    <activity name="Shop">
    </activity>

    <!--底部导航栏-->
    <activity name="BottomNavigation">

        <element name="BottomNavigation">
            <name>BottomNavigation</name>
            <type>LinearLayout</type>
            <pathtype>ID</pathtype>
            <pathvalue>fmc_ll_tab</pathvalue>
        </element>

        <element name="shop">
            <name>Shop</name>
            <type>button</type>
            <pathtype>ID</pathtype>
            <pathvalue>fmc_bn_shop</pathvalue>
        </element>

        <element name="vote">
            <name>vote</name>
            <type>button</type>
            <pathtype>ID</pathtype>
            <pathvalue>fmc_bn_vote</pathvalue>
        </element>

        <element name="photo">
            <name>photo</name>
            <type>ImageView</type>
            <pathtype>ID</pathtype>
            <pathvalue>fmc_iv_photo</pathvalue>
        </element>

        <element name="lookbook">
            <name>lookbook</name>
            <type>button</type>
            <pathtype>ID</pathtype>
            <pathvalue>fmc_bn_lookbook</pathvalue>
        </element>

        <element name="profile">
            <name>Profile</name>
            <type>button</type>
            <pathtype>ID</pathtype>
            <pathvalue>fmc_bn_profile</pathvalue>
        </element>

    </activity>

    <!--profile-->
    <activity name="profile">

        <element name="title">
            <name>title</name>
            <type>RelativeLayout</type>
            <pathtype>ID</pathtype>
            <pathvalue>profile_head</pathvalue>
        </element>

        <element name="SignIn">
            <name>SignIn</name>
            <type>button</type>
            <pathtype>ID</pathtype>
            <pathvalue>profile_sign_in</pathvalue>
        </element>

        <element name="user">
            <name>user</name>
            <type>TextView</type>
            <pathtype>ID</pathtype>
            <pathvalue>profile_user</pathvalue>
        </element>

    </activity>

    <!--login-->
    <activity name="login">

        <element name="title">
            <name>title</name>
            <type>TextView</type>
            <pathtype>ID</pathtype>
            <pathvalue>wab_tv_title</pathvalue>
        </element>

        <element name="mailAndPass">
            <name>mailAndPass</name>
            <type>EditText</type>
            <pathtype>CLASSNAME</pathtype>
            <pathvalue>android.widget.EditText</pathvalue>
        </element>

        <element name="signIn">
            <name>signIn</name>
            <type>button</type>
            <pathtype>ID</pathtype>
            <pathvalue>al_bn_sign</pathvalue>
        </element>
    </activity>
</page>